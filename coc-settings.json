{
  "list.normalMappings": {
    "<leader>u": "git.chunkUndo"
  },
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "explorer.icon.enableNerdfont": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "html",
    "scss",
    "vue",
    "json",
    "dart"
  ],
  "prettier.eslintIntegration": {
    "filePath": "./.eslintrc.json",
    "eslintPath": "./.eslintrc.js"
  },
  "eslint.autoFixOnSave": false,
  "eslint.autoFix": false,
  "html.filetypes": ["html", "handlebars", "htmldjango", "php"],
  "vetur.validation.template": true,
  "vetur.validation.templateProps": true,
  "vetur.validation.interpolation": true,
  "vetur.format.options.tabSize": 2,
  "eslint.enable": true,
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.format.defaultFormatter.scss": "prettier",
  "vetur.completion.autoImport": true,
  "vetur.completion.useScaffoldSnippets": true,
  "vetur.grammar.customBlocks": {},
  "eslint.options": { "configFile": "./.eslintrc.js" },
  "vetur.trace.server": "verbose",
  "vetur.dev.vlsPath": "~/.local/share/vim-lsp-settings/servers/vls/vls",
  "eslint.run": "onSave",
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "git.enableGutters": true,
  "suggest.noselect": false,
  "flutter.enabled": true,
  "languageserver": {
    "dart": {
      "command": "dart",
      "args": [
        "/usr/local/opt/dart/libexec/bin/snapshots/analysis_server.dart.snapshot",
        "--lsp"
      ],
      "filetypes": ["dart"],
      "disableDynamicRegister": true,
      "trace.server": "verbose"
    }
  }
}

{
  "emmet.includeLanguages": {"vue-html": "html", "javascript": "javascriptreact"},
  "emmet.optimizeStylesheetParsing": true,
  "languageserver": {
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php"],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    }
}}
